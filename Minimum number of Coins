// User function Template for Java

class Solution {
    static List<Integer> minPartition(int N) {
        // code here
        List<Integer> lt = new ArrayList<>();
        int[] arr = {1,2,5,10,20,50,100,200,500,2000};
        for(int i = arr.length-1; i>=0; i--){
            if(arr[i] > N){
                continue;
            }
            while(N-arr[i] >= 0){
                lt.add(arr[i]);
                N-=arr[i];
            }
        }
        return lt;
    }
}
