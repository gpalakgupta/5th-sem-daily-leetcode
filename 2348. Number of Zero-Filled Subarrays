class Solution {
    public long zeroFilledSubarray(int[] nums) {
        int n = nums.length;
        int st = 0;
        int end = 0;
        long ans = 0;
        while(end < n){
            int el = nums[end];
            if(el == 0){
                ans += (end-st)+1;
            }
            else{
                st = end+1;
            }
            end++;
        }
        return ans;
    }
}
