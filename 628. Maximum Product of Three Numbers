class Solution {
    public int maximumProduct(int[] nums) {
        int n = nums.length;
        Arrays.sort(nums);
        if(n == 3){
            int e1 = nums[0]*nums[1]*nums[2];
        }
        int e2 = nums[n-1]*nums[n-2]*nums[n-3];
        int e3 = nums[0]*nums[1]*nums[n-1];
        return Math.max(e2,e3);
    }
}
