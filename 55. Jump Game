class Solution {
    public boolean canJump(int[] nums) {
        int n = nums.length;
        Boolean[] dp = new Boolean[n];
        return helper(0,nums,dp);
    }
    public boolean helper(int idx, int[] nums, Boolean[] dp){
        if(idx >= nums.length-1){
            return true;
        }
        if(dp[idx] != null){
            return dp[idx];
        }
        int el = nums[idx];
        for(int i = 1; i<=el; i++){
            int st = idx+i;
            boolean ans = helper(st,nums,dp);
            if(ans){
               return dp[idx] = ans;
            }
        }
        return dp[idx] = false;
    }
}
