class Solution {
    public String frequencySort(String s) {
        HashMap<Character,Integer> map = new HashMap<>();
        for(char ch : s.toCharArray()){
            map.put(ch,map.getOrDefault(ch,0)+1);
        }

        List<Character> st = new ArrayList<>(map.keySet());
        Collections.sort(st,(a,b)-> map.get(b)-map.get(a));
        StringBuilder sb = new StringBuilder();
        for(char key : st){
            int n = map.get(key);
            for(int i = 0; i<n; i++){
                sb.append(key);
            }
        }
        return sb.toString();
    }
}
