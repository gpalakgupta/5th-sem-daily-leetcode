class Solution {
    public boolean lemonadeChange(int[] bills) {
        int sum = 0;
        int a = 0;
        int b = 0;
        int c = 0;
        for(int el : bills){
            if(el == 5){
                a++;
            }
            else if(el == 10){
                b++;
                a--;
            }
            else{
                if(b != 0 && b != 0){
                    b--;
                    a--;
                }
                else if(a >= 3) {
                    a-=3;
                }
                else{
                    return false;
                }
            }
            if(a<0 || b<0){
                return false;
            }
        }
        return true;
    }
}
