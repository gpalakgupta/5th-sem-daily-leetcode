class Solution {
    public long maximumSubarraySum(int[] nums, int k) {
        int st = 0;
        int end = 0;
        int n =  nums.length;
        HashSet<Integer> st1 = new HashSet<>();
        long ans = 0;
        long sum = 0;
        while(end < n){
            int el = nums[end];
            while(st1.contains(el)){
                st1.remove(nums[st]);
                sum-=nums[st];
                st++;
            }
            sum+=el;
            st1.add(el);
            if(st1.size() == k){
                ans = Math.max(ans,sum);
                sum-=nums[st];
                st1.remove(nums[st]);
                st++;
            }
            end++;
        }
        return ans;
    }
}
